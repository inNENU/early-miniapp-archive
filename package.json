{
  "name": "mynenu",
  "version": "2.4.1",
  "scripts": {
    "commit": "git cz",
    "ts:watch": "tsc --watch",
    "ts:compile": "tsc",
    "lint": "prettier --check --write **/*.ts && prettier --check --write --parser babel-flow **/*.wxs && prettier --check --write --parser html **/*.wxml && eslint --fix --ext .ts,.wxs . && stylelint --fix **/*.wxss",
    "lint:check": "prettier --check app/**/*.ts && prettier --check --parser babel-flow **/*.wxs && prettier --check --parser html **/*.wxml && eslint --ext .ts,.wxs . && stylelint **/*.wxss",
    "doc": "typedoc --options ./typedoc.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mister-Hope/inNENU.git"
  },
  "author": "Mr.Hope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mister-Hope/inNENU/issues"
  },
  "homepage": "https://mrhope.site",
  "dependencies": {
    "miniprogram-api-typings": "2.10.4",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@starptech/prettyhtml": "0.10.0",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "@typescript-eslint/typescript-estree": "2.29.0",
    "conventional-changelog-cli": "2.0.31",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "lint-staged": "10.1.7",
    "prettier": "2.0.5",
    "stylelint": "13.3.3",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.0.0",
    "typedoc": "0.17.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
