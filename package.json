{
  "name": "mynenu",
  "version": "2.4.1",
  "private": true,
  "workspaces": [
    "app/",
    "server/"
  ],
  "scripts": {
    "commit": "git cz",
    "ts:watch": "tsc --watch",
    "ts:compile": "tsc",
    "lint": "prettier --check --write . && prettier --check --write --parser babel-flow **/*.wxs && prettier --check --write --parser html **/*.wxml && eslint --fix --ext .ts,.wxs . && stylelint --fix **/*.wxss",
    "lint:check": "prettier --check app/**/*.ts **/*.json && prettier --check --parser babel-flow **/*.wxs && prettier --check --parser html **/*.wxml && eslint --ext .ts,.wxs . && stylelint **/*.wxss",
    "doc": "typedoc --options ./typedoc.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
    "upload": "yarn run upload:page && yarn run upload:function",
    "upload:page": "yarn workspace inenu-server upload:page",
    "upload:function": "yarn workspace inenu-server upload:function"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mister-Hope/inNENU.git"
  },
  "author": "Mr.Hope",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mister-Hope/inNENU/issues"
  },
  "homepage": "https://mrhope.site",
  "dependencies": {
    "miniprogram-api-typings": "2.11.0",
    "typescript": "3.9.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "@typescript-eslint/typescript-estree": "3.1.0",
    "conventional-changelog-cli": "2.0.34",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.9",
    "prettier": "2.0.5",
    "stylelint": "13.6.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "typedoc": "0.17.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "hooks": {
    "pre-commit": "lint-staged"
  }
}
