<!-- 导航栏 -->
<template name="head">
  <block wx:if="{{!hidden}}">
    <template is="{{T}}Head" data="{{title,leftText,titleDisplay,borderDisplay,shadow,action,statusBarHeight}}" />
  </block>
  <!-- share组件替换 -->
  <block wx:if="{{shareable||feedback||contact}}">
    <view class="shareIconCtn" style="top:{{top}}px;left:{{left}}px;" hover-class="shareHover" hover-stay-time="200" catchtouchstart="cA" catchtouchmove="cA" catchtouchend="cA" data-action="share" data-role="button">
      <image class="shareIcon" src="/components/common/share/share.svg" />
    </view>
    <view wx:if="{{menuDisplay}}" class="shareMask {{menuDisplay?'fadeIn':'fadeOut'}}" style="top:{{menuDisplay?'0':'100%'}};"></view>
    <view class="shareLayer {{menuDisplay?'moveUpIn':'moveUpOut'}}" catchtap="cA" data-action="share">
      <view class="shareBtnCtn">
        <button wx:if="{{shareable}}" class="shareBtn" hover-class="shareBtnHover" hover-start-time="0" plain="true" open-type="share">
          分享{{env==='wx'?'到微信':'给好友/分享到空间'}}
        </button>
        <button wx:if="{{shareable}}" class="shareBtn" hover-class="shareBtnHover" hover-start-time="0" plain="true" data-role="download">
          保存页面二维码
        </button>
        <button wx:if="{{contact}}" class="shareBtn" hover-class="shareBtnHover" hover-start-time="0" plain="true" open-type="contact" data-role="contact">
          联系开发者
        </button>
        <button wx:if="{{feedback}}" class="shareBtn" hover-class="shareBtnHover" hover-start-time="0" plain="true" open-type="feedback" data-role="feedback">
          意见反馈
        </button>
        <button class="cancelBtn" hover-class="shareBtnHover">取消</button>
      </view>
    </view>
  </block>
</template>
<!-- 文字段落 -->
<template name="p">
  <view wx:if="{{head}}" class="{{T}}head">{{head}}</view>
  <view class="P {{T}}P" style="text-align:{{align?align:''}};">
    <text style="{{style}}" selectable="true" space="ensp" decode="true">{{text}}</text>
    <block wx:if="{{src}}">
      <template is="{{T}}img" data="{{src,load,error,id,imgMode,lazy,desc}}" />
    </block>
  </view>
</template>
<!-- 图片 -->
<template name="img">
  <template is="{{T}}img" data="{{src,load,error,id,imgMode,lazy,desc}}" />
</template>
<!-- 网格 -->
<template name="grid">
  <template is="{{T}}grid" data="{{head,content,foot}}" />
</template>
<!-- 错误提示 -->
<template name="error">
  <template is="{{T}}error" data="{{action,statusBarHeight}}" />
</template>
