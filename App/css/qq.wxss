/* Andriod grid */
.AndroidGridHead {
  text-align: left;
  min-height: 37px;
  padding: 23px 25px 5px 25px;
}

.AndriodGridList {
  display: flex;
  flex-wrap: wrap;
  padding: 0 15px;
  text-align: left;
}

.AndriodGridbox {
  padding: 15px;
  border-radius: 6px;
  width: 45%;
  margin: 0 2.5% 20px;
  background-image: url(data:image/png;base64,);
  background-size: cover;
  background-position: center;
  position: relative;
  z-index: 1;
}

.AndriodGridbox::after {
  content: "";
  position: absolute;
  z-index: -1;
  background-color: inherit;
  width: 100%;
  height: 100%;
  left: 0;
  bottom: -10%;
  border-radius: 5px;
  opacity: 0.2;
  transform: scale(0.9, 0.9);
}

.AndriodGridbox.cur {
  color: #fff;
  background: rgb(94, 185, 94);
  box-shadow: 2px 2px 3px rgba(94, 185, 94, 0.4);
}

@media (min-width:768px) {
  .AndriodGridList {
    padding: 0 10px;
  }
}

@media (min-width:480px) {
  .AndriodGridbox {
    margin: 0 10px 20px;
    width: calc(25% - 20px);
  }
}

@media (min-width:1024px) {
  .AndriodGridbox {
    width: calc(16.666% - 20px);
    margin: 0 10px 20px;
  }
}

.AndriodGridText {
  font-size: 16px;
  font-weight: 300;
}

.AndriodGridText::first-letter {
  font-size: 20px;
  margin-right: 2px;
}

.AndriodGridName {
  font-size: 15px;
  text-transform: Capitalize;
  margin-top: 10px;
  position: relative;
}

.AndriodGridName::before {
  content: "";
  position: absolute;
  display: block;
  width: 20px;
  height: 3px;
  background: #fff;
  bottom: 0;
  right: 0;
  opacity: 0.5;
}

.AndriodGridName::after {
  content: "";
  position: absolute;
  display: block;
  width: 50px;
  height: 1px;
  background: #fff;
  bottom: 0;
  right: 20px;
  opacity: 0.3;
}

.AndriodGridName::first-letter {
  font-weight: bold;
  font-size: 18px;
  margin-right: 1px;
}

.AndriodGridbox .gridIcon {
  position: absolute;
  right: 15px;
  top: 15px;
  font-size: 26px;
  width: 30px;
  height: 30px;
  text-align: center;
  line-height: 30px;
}