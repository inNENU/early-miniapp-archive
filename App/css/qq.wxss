/* grid */

.gridHead {
  font-size: 14px;
  color: #999;
  text-align: left;
  font-weight: 500;
  min-height: 37px;
  padding: 23px 15px 5px 15px;
}

.gridCtn {
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
  position: relative;
}

.gridCtn:before {
  content: " ";
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
  color: #cac9ce;
  top: 0;
  border-top: 1px solid #cac9ce;
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  -webkit-transform: scaleY(0.4);
  transform: scaleY(0.4);
}

.nm .gridCtn {
  background-color: #1C1C1E;
}

.nm .gridCtn:before {
  border-color: #3D3D41
}

.gridBox {
  width: 25%;
  padding: 0 !important;
  margin: 0 !important;
  border: none !important;
  border-radius: 0 !important;
  line-height: 1.6 !important;
  position: relative;
}

.gridBox:before,
.gridBox:after {
  content: " ";
  position: absolute;
  right: 0;
  bottom: 0;
  color: #cac9ce;
}

.gridBox:before {
  top: 0;
  width: 1px;
  border-right: 1px solid #cac9ce !important;
  -webkit-transform-origin: 100% 0;
  transform-origin: 100% 0;
  -webkit-transform: scaleX(0.4);
  transform: scaleX(0.4);
}

.gridBox:after {
  left: 0;
  height: 1px;
  border-bottom: 1px solid #cac9ce !important;
  -webkit-transform-origin: 0 100%;
  transform-origin: 0 100%;
  -webkit-transform: scaleY(0.4);
  transform: scaleY(0.4);
}

.nm .gridBox:before,
.nm .gridBox:after {
  border-color: #3D3D41 !important;
}

.gridIcon {
  width: 44px;
  height: 44px;
  margin-top: 16px;
}

.gridTitle {
  font-size: 16px;
  color: #444;
  margin-bottom: 6px;
}

.nm .gridTitle {
  color: #bbb;
}

.gridFoot {
  font-size: 14px;
  color: #999;
  text-align: left;
  padding: 3px 15px;
  min-height: 8px;
}

.nm .gridFoot {
  color: #666;
}

@media (min-width:480px) {
  .gridBox {
    width: 16.666%;
  }
}

@media (min-width:1024px) {
  .gridBox {
    width: 12.5% - 1rpx;
  }
}



