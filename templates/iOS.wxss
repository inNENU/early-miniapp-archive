/* ---iOS theme made by Mr.Hope--- */

.iOS, .iOSgrey {
  width: 100%;
  min-height: 100%;
  text-align: center;
}

.iOS:last-child, .iOSgrey:last-child {
  padding-bottom: 40px;
}

.iOS {
  background-color: #fff;
}

.iOSgrey {
  background-color: #efeef4;
}

.nm.iOSgrey {
  background-color: #10110b;
}

/* iOS Head */

.iOS-navigationBar, .iOS-navPlaceHolder {
  width: 100%;
  background-color: #f5f5f7;
  text-align: center;
}

.iOS-navigationBar {
  height: 44px;
  padding-top: 20px;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  position: fixed;
  top: 0;
  z-index: 99;
}

.iOS-navPlaceHolder {
  height: 64px;
  font-size: 10px;
  color: #acacb0;
}

.nm .iOS-navigationBar, .nm .iOS-navPlaceHolder {
  background-color: #0a0a08;
}

.nm .iOS-navPlaceHolder {
  color: #555;
}

.iOS-navBack {
  min-width: 0 !important;
  min-height: 0 !important;
  padding: 0 5px !important;
  border: none !important;
  background-color: rgba(0, 0, 0, 0);
  margin: 0 3%;
  line-height: 2.4;
  position: relative;
  display: -webkit-flex;
  display: flex;
  align-items: center;
}

.iOS-backArrow {
  width: 12px;
  height: 12px;
  border-width: 0 0 3px 3px;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  border-color: #3478f7;
}

.iOS-nav-text {
  color: #3478f7;
  font-size: 16px;
  z-index: 101;
}

.iOS-titletext {
  width: 100%;
  position: absolute;
  font-size: 17px;
  font-weight: 500;
  z-index: 100;
}

.nm .iOS-titletext, .nm .iOS-head, .nm .iOSH3, .nm .iOSH4, .iOS.nm .iOShead,
.nm .iOSLinkText, .nm .iOSpickerViewCtn {
  color: #fff;
}

.iOSnavDl, .iOS-head {
  border-bottom: 1rpx #acacb0 solid;
}

.iOSnavDl {
  width: 100%;
  height: 0;
  position: fixed;
  top: 64px;
  z-index: 100;
}

.nm .iOSnavDl, .nm .iOS-head {
  border-bottom: 1rpx #53534f solid;
}

.iOS-head {
  padding: 3px 5%;
  text-align: left;
  font-size: 34px;
  font-weight: 500;
  background-color: #f4f4f4;
}

.nm .iOS-head {
  background-color: #0c0c0c;
}

/* iOS content */

.iOSH3, .iOSLinkText, .iOShead, .iOSP, .iOSfoot {
  text-align: left;
}

.iOSH3, .iOS .iOShead {
  font-weight: 500;
  padding: 9px 5% 3px 5%;
}

.iOSH3 +.iOShead, .iOSH3 +.iOSH3 {
  padding-top: 2px;
}

.iOSgrey .iOShead {
  min-height: 9px;
  padding: 23px 15px 5px 15px;
}

.iOSgrey .iOSP {
  font-size: 17px;
  padding: 10px 15px;
}

.iOSH3 {
  min-height: 24px;
  font-size: 22px;
}

.iOS .iOShead {
  min-height: 20px;
  font-size: 19px;
}

.iOSP, .iOSLctn {
  background-color: #fefefe;
}

.iOS .iOSP {
  font-size: 16px;
  padding: 0 15px;
}

.iOSgrey.nm .iOSP {
  border-color: #353631;
}

.iOSLctn, .iOSgrey .iOSP {
  border-top: 1rpx #cac9ce solid;
  border-bottom: 1rpx #cac9ce solid;
}

.iOSfoot, .iOSgrey .iOShead {
  font-size: 13px;
  color: #666;
}

.iOSfoot {
  padding: 5px 15px 0 15px;
}

.nm .iOSfoot, .iOSgrey.nm .iOShead {
  color: #999;
}

.iOSLctn {
  /* font-size: 17px;
  line-height: 1.41176471; */
}

.nm .iOSLctn {
  border-color: #353631;
}

.iOS-listCtn {
  padding: 10px 15px;
  font-size: 17px;
  /* font-size: 16px; */
  line-height: 1.45;
  /* line-height: 1.4; */
  border-radius: 0;
  border: none !important;
  color: #000 !important;
}

.iOS-listCtn:empty {
  padding: 0;
}

.nm .iOS-listCtn {
  background-color: #000;
  color: #fff !important;
}

.iOS-listIcon {
  width: 18px;
  height: 18px;
  margin-right: 4px;
}

.iOS-switchList {
  padding: 6px 15px;
}

.iOS-list-Container {
  position: relative;
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
}

.iOS-list-Container, .iOSLinkText, .iOSpick-view-column:first-child,
.iOSpick-view-column:last-child {
  -webkit-flex: 1;
  flex: 1;
}

.iOS-linkDesciption {
  color: #999 !important;
  text-align: right;
}

.iOSaccess {
  padding-right: 15px;
  position: relative;
}

.iOSaccess:after {
  content: " ";
  display: inline-block;
  width: 7px;
  height: 7px;
  border-width: 2px 2px 0 0;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -5px;
  right: 2px;
  border-color: #c7c7cc;
}

.nm .iOSaccess:after {
  border-color: #383833;
}

.iOS-link-divline {
  width: 96%;
  height: 0.45px;
  margin-left: 4%;
  background-color: #cac9ce;
}

.nm .iOS-link-divline {
  background-color: #353631;
}

.iOS-link-divline:last-child {
  display: none;
}

.iOS-img, .iOSP-img {
  border-radius: 8px;
}

.iOS-img {
  width: 90% !important;
  margin: 4px 5%;
}

.iOSP-img {
  width: 100% !important;
  margin-top: 2px;
  flex: auto;
}

.iOSN {
  font-size: 12px;
  font-weight: 300;
  margin-top: -2px;
  margin-bottom: 4px;
  text-align: center;
  color: #222;
}

.nm .iOSN {
  color: #ddd;
}

.iOSpickerViewCtn {
  height: 160px;
}

.iOSpick-view-column {
  min-width: 50px;
  flex: none;
}

.iOSpickColumn {
  display: -webkit-flex;
  display: flex;
  padding: 0 8px;
  flex-direction: column;
  justify-content: center;
  align-items: stretch;
  text-align: center;
  font-size: 18px;
  flex: none;
}

.iOSpick-view-column:first-child .iOSpickColumn {
  text-align: right;
}

.iOSpick-view-column:last-child .iOSpickColumn {
  text-align: left;
}

.nm.iOS, .nm .iOSP, .nm .iOSLctn, .nm .iOSpickerViewCtn,
.nm .iOSpick-view-column {
  background-color: #000;
  color: #fff;
}

.iOSsliderCtn {
  padding: 5px 15px;
}

