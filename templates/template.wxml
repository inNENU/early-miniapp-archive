</template>

<template name='list'>
  <view wx:if="{{head!==false}}" class='commonhead'>{{head}}</view>
  <view class='Lctn'>
    <block wx:for="{{content}}" wx:for-item="List" wx:key="text">
      <view wx:if='{{List.display!==false&&List.url}}' id="{{List.id}}" class="listCtn" hover-class="buttonHover" hover-stay-time='100' catchtap="cA" data-action='navigate' data-url='{{List.url}}' data-aim="{{List.aim}}">
        <view class='list-Container'>
          <image wx:if="{{List.icon}}" class='listIcon' src='{{List.icon}}' data-icon='true' />
          <view class='LinkText'>{{List.text}}</view>
          <view class="linkDesciption {{List.url?'access':''}}">{{List.desc}}</view>
        </view>
      </view>
      <button wx:elif='{{List.display!==false&&(List.inlay||List.button||List.slider)}}' id="{{List.id}}" class="listCtn" hover-class="buttonHover" hover-stay-time='100' catchtap="{{List.pickerValue&&!List.picker?'cA':''}}{{List.pickerValue&&List.picker?'List.picker':''}}{{List.button?List.button:''}}{{List.slider?List.slider:''}}"
        data-id="{{List.id}}" data-action='picker' plain='true' disabled='{{List.disabled}}'>
        <view class='list-Container'>
          <image wx:if="{{List.icon}}" class='listIcon' src='{{List.icon}}' data-icon='true' />
          <view class='LinkText' style="{{List.button?'color:#3478f7;':''}}">{{List.text}}</view>
          <view wx:if='{{List.slider}}' class='linkDesciption'>{{List.value}}</view>
          <view wx:elif='{{List.pickerValue}}' class='linkDesciption'>
            <block wx:if="{{List.single}}">{{List.value}}</block>
            <block wx:else>
              <block wx:for="{{List.value}}" wx:key="*this">{{item}}</block>
            </block>
          </view>
        </view>
      </button>
      <picker wx:elif="{{List.display!==false&&List.pickerValue}}" id="{{List.id}}" class='listCtn' hover-class="buttonHover" hover-stay-time='100' bindchange="{{List.picker?List.picker:'cA'}}" value="{{List.currentValue}}" range="{{List.pickerValue}}" data-id="{{List.id}}"
        data-action='picker'>
        <view class='list-Container'>
          <view class='LinkText'>{{List.text}}</view>
          <view wx:if='{{List.pickerValue}}' class='linkDesciption'>
            <block wx:for='{{List.value}}' wx:key="*this">{{item}}</block>
          </view>
        </view>
      </picker>
      <navigator wx:elif='{{List.display!==false&&List.navigate}}' id="{{List.id}}" class="listCtn" hover-class="buttonHover" hover-stay-time='100' url='{{List.navigate}}' openType='{{List.openType}}' target='{{List.target}}'>
        <view class='list-Container'>
          <image wx:if="{{List.icon}}" class='listIcon' src='{{List.icon}}' data-icon='true' />
          <view class='LinkText'>{{List.text}}</view>
          <view class="linkDesciption {{List.url?'access':''}}">{{List.desc}}</view>
        </view>
      </navigator>
      <view wx:elif="{{List.display!==false}}" id="{{List.id}}" class="listCtn">
        <image wx:if="{{List.icon}}" class='listIcon' src='{{List.icon}}' data-icon='true' />
        <view wx:if="{{List.display!==false}}" class='list-Container'>
          <view class='LinkText'>
            <text selectable='true' space='ensp' decode='true'>{{List.text}}</text>
          </view>
          <view class="linkDesciption">{{List.desc}}
            <switch wx:if='{{List.swiKey}}' checked='{{List.status}}' bindchange="{{List.Switch?List.Switch:'cA'}}" data-id="{{List.id}}" data-action='switch' />
          </view>
        </view>
      </view>
      <block wx:if='{{List.visible}}'>
        <view class='link-divline'></view>
        <view wx:if="{{List.slider}}" class='sliderCtn'>
          <slider min="{{List.min?List.min:'0'}}" max="{{List.max?List.max:'100'}}" value='{{List.value}}' bindchanging="{{List.slider?List.slider:''}}" step="{{List.step?List.step:'1'}}" bindchange="{{List.slider?List.slider:''}}" data-id="{{List.id}}" data-action='slider'
          />
        </view>
        <picker-view wx:if="{{List.pickerValue}}" indicator-class='pickSelected' class='pickerViewCtn' value="{{List.currentValue}}" bindchange="cA" data-id="{{List.id}}" data-action='picker' mask-class='pickerMask'>
          <block wx:if="{{List.single}}">
            <picker-view-column class='pick-view-column'>
              <view wx:for='{{List.pickerValue}}' wx:key="*this" class='pickColumn'>{{item}}</view>
            </picker-view-column>
          </block>
          <block wx:else>
            <block wx:for='{{List.pickerValue}}' wx:key="*this">
              <picker-view-column class='pick-view-column'>
                <view wx:for="{{item}}" wx:key="*this" class='pickColumn'>{{item}}</view>
              </picker-view-column>
            </block>
          </block>
        </picker-view>
      </block>
      <view wx:if="{{List.display!==false}}" class="{{List.icon?'listIcon ':''}}link-divline"></view>
    </block>
  </view>
  <view wx:if="{{foot}}" class='commonfoot'>{{foot}}</view>
</template>

<template name='doc'>
  <button id="{{id}}" class='docCtn' hover-class="buttonHover" hover-stay-time='100' catchtap='cA' data-url="{{url}}" data-action='doc'>
    <image class='docIcon' mode='widthFix' src='/icon/{{docType}}.svg' />
    <view class='docname docAccess'>{{docName}}.{{docType}}</view>
  </button>
</template>

<template name='phone'>
  <view class='phoneCtn' id='{{id}}' catchtap='cA' data-action='phone'>
    <image class='phoneIcon' mode='widthFix' src='/icon/phone.svg' />
    <view class='phoneNum'>{{num}}</view>
    <view class='phoneCallCtn' hover-class='buttonHover' hover-stay-time='100'>
      <image class='phoneCall' mode='widthFix' src='/icon/call.svg' data-type="call" />
    </view>
    <view class='phoneAddCtn' hover-class='buttonHover' hover-stay-time='100'>
      <image class='phoneAdd' mode='widthFix' src='/icon/add.svg' data-type="add" />
    </view>
  </view>
</template>

<template name='grid'>
  <view wx:if="{{head!==false}}" class='gridHead'>{{head}}</view>
  <view class='gridCtn'>
    <view wx:for="{{content}}" wx:for-item="List" wx:key="text" class='gridBox' hover-class='buttonHover' hover-stay-time='100' catchtap="cA" data-action='navigate' data-url='{{List.url}}' data-aim="{{List.aim}}">
      <image class='gridIcon' src="{{List.icon}}" />
      <view class='gridTitle'>{{List.text}}</view>
    </view>
  </view>
  <view wx:if="{{foot}}" class='gridFoot'>{{foot}}</view>
</template>

<template name='swiper'>
  <swiper class="{{Class?Class:'swiper'}}" style="{{style}}" indicator-dots="{{indicatorDots==false?'':true}}" indicator-color="{{dotColor?dotColor:'#ffffff88'}}" indicator-active-color="{{dotActiveColor?dotActiveColor:'#fff'}}" autoplay="{{autoplay==false?'':true}}"
    interval="{{interval?interval:'5000'}}" duration="{{duration?duration:'500'}}" circular="{{circular==false?'':true}}" vertical="{{vertical?true:''}}" previous-margin="{{preMargin?preMargin:'0px'}}" next-margin="{{nextMargin?nextMargin:'0px'}}" bindchange="{{change?change:''}}"
    bindanimationfinish="{{animation?animation:''}}" data-action='swiper'>
    <swiper-item wx:for="{{url}}" wx:key="*this" wx:for-item='url'>
      <image class="{{imgClass?imgClass:'swiperImg'}}" src="{{url}}" mode="aspectFill" />
    </swiper-item>
  </swiper>
</template>

<template name="audio">
  <audio class="audio" id="{{id}}" poster='{{poster}}' src='{{src}}' name='{{name}}' author='{{author}}' loop="{{loop}}" controls="{{controls==false?false:true}}" />
</template>

<template name="video">
  <video class="video" id="{{id}}" src='{{src}}' controls="{{controls==false?false:true}}" loop="{{loop}}" autoplay="{{autoplay}}" initial-time='{{startTime}}' duration='{{duration}}' danmu-btn='{{danmuBtn}}' danmu-list="{{danmuList}}" enable-danmu="{{danmuList&&danmuBtn?true:false}}"
    poster="{{poster}}" bindplay='cA' bindwaiting="cA" binderror="cA" data-action='video' />
</template>

<template name='gzh'>
