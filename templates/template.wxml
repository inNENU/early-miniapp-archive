<!-- NENU theme -->

<template name='NENUhead'>
  <block wx:if="{{item.display!=false}}">
    <view wx:if="{{item.action}}" class='NENUnav' style="background-color:{{item.color}};padding-top:{{item.statusBarHeight}}px;">
      <view class='NENU-navText'>{{item.title}}</view>
    </view>
    <view wx:else class='NENUnav2' style='background-color:{{item.color}};height:{{item.statusBarHeight}}px;'>
      <view class='backBtn' style="top:{{item.statusBarHeight+5.5}}px;" hover-class='buttonHover' catchtap='cA' data-action='back'>
        <image class='backIcon' src='/icon/back.svg' />
      </view>
      <view class="NENUTitle {{item.titleDisplay?'fadeOut':'fadeIn'}}" style='top:{{item.statusBarHeight+5.5}}px;'>{{item.title}}</view>
    </view>
    <view style='width:100%;background-color:{{item.color}};height:{{item.action?item.statusBarHeight+40:item.statusBarHeight}}px;'></view>
  </block>
</template>

<template name='NENUh3'>
  <view class='NENUH3' style="{{item.style}}">{{item.text}}</view>
</template>

<template name='NENUp'>
  <view wx:if="{{item.head}}" class='NENUH3'>{{item.head}}</view>
  <view class='NENUP'>
    <text style='{{item.style}}'>{{item.text}}</text>
    <view wx:if="{{item.src&&item.error}}" class='iOSImgError'>
      <icon type="warn" style='margin-right:5px;' />
      <text class='grey'>图片加载失败</text>
    </view>
    <view wx:elif="{{item.src&&!item.load}}" class='iOSImgError'>
      <image class='loadingImg' mode="{{item.imgMode}}" src="/icon/loading.svg" />
      <text class='grey'>加载中...</text>
    </view>
    <view class='flexbox' style="{{item.load?'':'display: none;'}}">
      <image id="{{item.id}}" class='iOSP-img' mode="{{item.imgMode}}" lazy-load="{{item.lazy!==false}}" src="{{item.src}}" bindload="cA" binderror="cA" catchtap="cA" data-action='img' />
    </view>
  </view>
</template>

<template name='NENUlist'>
  <view wx:if="{{item.head!==false}}" class='NENUH3'>{{item.head}}</view>
  <view class='iOSLctn'>
    <block wx:for="{{item.content}}" wx:for-item="List" wx:key="{{List.text}}">
      <navigator wx:if='{{List.url}}' id="{{List.id}}" class="iOS-listCtn" url='{{List.url}}'>
        <view class='iOS-list-Container'>
          <image wx:if="{{List.icon}}" class='iOS-listIcon' src='{{List.icon}}' data-icon='true' />
          <view class='iOSLinkText'>{{List.text}}</view>
          <view class="iOS-linkDesciption {{List.url?'iOSaccess':''}}">{{List.desc}}</view>
        </view>
      </navigator>
      <button wx:elif='{{List.display!==false&&(List.inlay||List.button||List.slider)}}' id="{{List.id}}" class="iOS-listCtn" hover-class="buttonHover" catchtap="{{List.pickerValue&&!List.picker?'cA':''}}{{List.pickerValue&&List.picker?'List.picker':''}}{{List.button?List.button:''}}{{List.slider?List.slider:''}}"
        data-id="{{List.id}}" data-action='picker' plain='true' open-type='{{List.open-type}}' disabled='{{List.disabled}}'>
        <view class='iOS-list-Container'>
          <image wx:if="{{List.icon}}" class='iOS-listIcon' src='{{List.icon}}' data-icon='true' />
          <view class='iOSLinkText' style="{{List.button?'color:#3478f7;':''}}">{{List.text}}</view>
          <view wx:if='{{List.slider}}' class='iOS-linkDesciption'>{{List.value}}</view>
          <view wx:elif='{{List.pickerValue}}' class='iOS-linkDesciption'>
            <block wx:if="{{List.single}}">{{List.value}}</block>
            <block wx:else>
              <block wx:for="{{List.value}}" wx:key="*this">{{item}}</block>
            </block>
          </view>
        </view>
      </button>
      <picker wx:elif="{{List.display!==false&&List.pickerValue}}" id="{{List.id}}" class='iOS-listCtn' hover-class="buttonHover" bindchange="{{List.picker?List.picker:'cA'}}" value="{{List.currentValue}}" range="{{List.pickerValue}}" data-id="{{List.id}}"
        data-action='picker'>
        <view class='iOS-list-Container'>
          <view class='iOSLinkText'>{{List.text}}</view>
          <view wx:if='{{List.pickerValue}}' class='iOS-linkDesciption'>
            <block wx:for='{{List.value}}' wx:key="*this">{{item}}</block>
          </view>
        </view>
      </picker>
      <view wx:elif="{{List.display!==false}}" id="{{List.id}}" class="iOS-listCtn">
        <image wx:if="{{List.icon}}" class='iOS-listIcon' src='{{List.icon}}' data-icon='true' />
        <view wx:if="{{List.display!==false}}" class='iOS-list-Container'>
          <view class='iOSLinkText'>{{List.text}}</view>
          <view class="iOS-linkDesciption">{{List.desc}}
            <switch wx:if='{{List.swiKey}}' checked='{{List.status}}' bindchange="{{List.Switch?List.Switch:'cA'}}" data-id="{{List.id}}" data-action='switch' />
          </view>
        </view>
      </view>
      <block wx:if='{{List.visible}}'>
        <view class='iOS-link-divline'></view>
        <view wx:if="{{List.slider}}" class='iOSsliderCtn'>
          <slider min="{{List.min?List.min:'0'}}" max="{{List.max?List.max:'100'}}" value='{{List.value}}' bindchanging="{{List.slider?List.slider:''}}" step="{{List.step?List.step:'1'}}" bindchange="{{List.slider?List.slider:''}}" data-id="{{List.id}}" data-action='slider'
          />
        </view>
        <picker-view wx:if="{{List.pickerValue}}" indicator-class='iOSpickSelected' class='iOSpickerViewCtn' value="{{List.currentValue}}" bindchange="cA" data-id="{{List.id}}" data-action='picker'>
          <block wx:if="{{List.single}}">
            <picker-view-column class='iOSpick-view-column'>
              <view wx:for='{{List.pickerValue}}' wx:key="*this" class='iOSpickColumn'>{{item}}</view>
            </picker-view-column>
          </block>
          <block wx:else>
            <block wx:for='{{List.pickerValue}}' wx:key="*this">
              <picker-view-column class='iOSpick-view-column'>
                <view wx:for="{{item}}" wx:key="*this" class='iOSpickColumn'>{{item}}</view>
              </picker-view-column>
            </block>
          </block>
        </picker-view>
      </block>
      <view wx:if="{{List.display!==false}}" class="{{List.icon?'iOS-listIcon ':''}}iOS-link-divline"></view>
    </block>
  </view>
  <view wx:if="{{item.foot!==false}}" class='iOSfoot'>{{item.foot}}</view>
</template>

<template name='NENUimg'>
  <view wx:if="{{item.error}}" class='iOSImgError'>
    <icon type="warn" size='32' style='margin-bottom:5px;' />
    <text class='grey'>图片加载失败</text>
  </view>
  <view wx:elif="{{!item.load}}" class='iOSImgError'>
    <image class='loadingImg' mode="{{item.imgMode}}" src="/icon/loading.svg" />
    <text class='grey'>加载中...</text>
  </view>
  <view class='flexbox' style="{{item.load?'':'display: none;'}}">
