    </navigator>
  </view>
  <view wx:if="{{item.foot}}" class='gridFoot'>{{item.foot}}</view>
</template>

<template name='iOSswiper'>
  <swiper class="{{item.Class?item.Class:'swiper'}}" style="{{item.style}}" indicator-dots="{{item.indicatorDots==false?'':true}}" indicator-color="{{item.dotColor?item.dotColor:'#ffffff88'}}" indicator-active-color="{{item.dotActiveColor?item.dotActiveColor:'#fff'}}"
    autoplay="{{item.autoplay==false?'':true}}" interval="{{item.interval?item.interval:'5000'}}" duration="{{item.duration?item.duration:'500'}}" circular="{{item.circular==false?'':true}}" vertical="{{item.vertical?true:''}}" previous-margin="{{item.preMargin?item.preMargin:'0px'}}"
    next-margin="{{item.nextMargin?item.nextMargin:'0px'}}" bindchange="{{item.change?item.change:''}}" bindanimationfinish="{{item.animation?item.animation:''}}" data-action='swiper'>
    <swiper-item wx:for="{{item.url}}" wx:key="*this" wx:for-item='url'>
      <image class="{{item.imgClass?item.imgClass:'swiperImg'}}" src="{{url}}" mode="{{item.imgMode?item.imgMode:'aspectFill'}}" />
    </swiper-item>
  </swiper>
</template>

<template name='iOSfoot'>
  <view class='Footer'>
