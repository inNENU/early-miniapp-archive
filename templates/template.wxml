    <swiper-item wx:for="{{item.url}}" wx:key="*this" wx:for-item='url'>
      <image class="{{item.imgClass?item.imgClass:'swiperImg'}}" src="{{url}}" mode="{{item.imgMode?item.imgMode:'aspectFill'}}" />
