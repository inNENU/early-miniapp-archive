.commonBg {
  width: 100%;
  height: 100%;
  background-color: #fff;
}

.nm.commonBg {
  background-color: #000;
}

/* loading */

.commonLoading {
  width: 100%;
  height: 100%;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  align-items: center;
}

.loadingIconCtn {
  width: 200rpx;
  height: 250rpx;
  -webkit-filter: drop-shadow(-1px 1px 2px rgba(191, 190, 195, 0.15));
  filter: drop-shadow(-1px 1px 2px rgba(191, 190, 195, 0.15));
}

.loadingIcon {
  width: 200rpx;
  height: 200rpx;
  position: relative;
}

@keyframes loading {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
    top: 0px;
  }

  10% {
    -webkit-transform: rotate(-7.2deg);
    transform: rotate(-7.2deg);
    top: -18rpx;
  }

  20% {
    -webkit-transform: rotate(-28.8deg);
    transform: rotate(-28.8deg);
    top: -32rpx;
  }

  30% {
    -webkit-transform: rotate(-64.8deg);
    transform: rotate(-64.8deg);
    top: -42rpx;
  }

  40% {
    -webkit-transform: rotate(-115.2deg);
    transform: rotate(-115.2deg);
    top: -48rpx;
  }

  50% {
    -webkit-transform: rotate(-180deg);
    transform: rotate(-180deg);
    top: -50rpx;
  }

  60% {
    -webkit-transform: rotate(-244.8deg);
    transform: rotate(-244.8deg);
    top: -48rpx;
  }

  70% {
    -webkit-transform: rotate(-295.2deg);
    transform: rotate(-295.2deg);
    top: -42rpx;
  }

  80% {
    -webkit-transform: rotate(-331.2deg);
    transform: rotate(-331.2deg);
    top: -32rpx;
  }

  90% {
    -webkit-transform: rotate(-352.8deg);
    transform: rotate(-352.8deg);
    top: -18rpx;
  }

  100% {
    -webkit-transform: rotate(-360deg);
    transform: rotate(-360deg);
    top: 0px;
  }
}

.loading {
  -webkit-animation: loading 2s linear 0s none infinite alternate-reverse;
  animation: loading 2s linear 0s none infinite alternate-reverse;
}

.loadingText {
  color: #888;
  font-size: 32rpx;
}

@keyframes shadow {
  0%, 100% {
    transform: scaleX(1.4);
  }

  50% {
    transform: scaleX(1);
  }

}

.loadingShadow {
  width: 120rpx;
  height: 20rpx;
  margin: -20rpx 40rpx 0 40rpx;
  border-radius: 50%;
  background: rgba(191, 190, 195, 0.15);
  box-shadow: 0 0 5px rgba(191, 190, 195, 0.15);
  -webkit-animation: shadow 2s ease 0s none infinite alternate-reverse;
  animation: shadow 2s ease 0s none infinite alternate-reverse;
}

/* Nav */

.navBackHover {
  opacity: 0.2;
}

.headCtn {
  height: 56px;
  background-color: #d9d9d9;
}

/* tab */

.tab {
  position: fixed;
  bottom: 0;
  z-index: 99;
  width: 94%;
  min-height: 80rpx;
  max-height: 100rpx;
  display: -webkit-flex !important;
  display: flex !important;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 3%;
  background-color: #fff;
  box-shadow: 0 -2px 5px rgba(219, 218, 223, 0.95);
}

.tabIconCtn {
  display: inline-flex;
  flex-direction: column;
}

.tabIcon {
  width: 70rpx;
  height: 70rpx;
}

.tabName {
  text-align: center;
  font-size: 10px;
  color: #aaa;
}

/* back */

.backBtn {
  position: fixed;
  z-index: 999;
  top: 26px;
  left: 9px;
  width: 46px;
  height: 31px;
  border-radius: 18px;
  background-color: rgba(0, 0, 0, 0.15);
  border: 1rpx solid rgba(255, 255, 255, 0.2);
}

.backIcon {
  width: 24px;
  height: 24px;
  margin: 4px auto;
}

/* close */

.closeBtn {
  position: fixed;
  z-index: 100;
  top: 26px;
  left: 9px;
  width: 31px;
  height: 31px;
  border-radius: 18px;
  background-color: rgba(0, 0, 0, 0.15);
  border: 1rpx solid rgba(255, 255, 255, 0.2);
}

.closeIcon {
  width: 24px;
  height: 24px;
  margin: 4px auto;
}

/* image */

.loadingImg {
  width: 60px;
  height: 60px;
  margin-bottom: 6px;
}

.imgError {
  width: 90%;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 8px 5%;
}

/* doc */

.docCtn {
  width: 100%;
  background-color: #fefefe;
  padding: 10px 0 !important;
  line-height: 2;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  border-left: none !important;
  border-right: none !important;
  border-top: 1rpx #cac9ce solid;
  border-bottom: 1rpx #cac9ce solid;
  border-radius: 0 !important;
}

.nm .docCtn {
  background-color: #10110b !important;
  color: #fff !important;
  border-top-color: #353631;
  border-bottom-color: #353631;
}

.docIcon {
  width: 32px;
  height: 32px;
  margin: 0 32px;
}

.docname {
  font-size: 18px;
  flex: 1 1 auto;
  text-align: left;
}

.docAccess {
  margin-right: 30px;
}

.docAccess:after {
  content: " ";
  display: inline-block;
  width: 7px;
  height: 7px;
  border-width: 2px 2px 0 0;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -5px;
  right: 17px;
  border-color: #c7c7cc;
}

.nm .docAccess:after {
  border-color: #383833;
}

/* phone */

.phoneCtn {
  display: -webkit-flex;
  display: flex;
  align-items: center;
  padding: 0 15px;
  line-height: 2.5;
  background-color: #fefefe;
}

.phoneIcon {
  width: 24px;
  height: 24px;
  margin-right: 15px;
}

.phoneCallCtn, .phoneCall {
  width: 32px;
  height: 32px;
}

.phoneAddCtn, .phoneAdd {
  width: 28px;
  height: 28px;
}

.phoneCallCtn, .phoneAddCtn {
  border-radius: 12px;
  margin: 0 4px;
}

.phoneNum {
  font-size: 20px;
  font-weight: 400;
  flex: 1 1 auto;
  text-align: left;
}

.nm .phoneCtn {
  background-color: #10110b !important;
  color: #fff !important;
}

/* grid */

.gridHead {
  font-size: 14px;
  color: #999;
  text-align: left;
  font-weight: 500;
  min-height: 9px;
  padding: 23px 15px 5px 15px;
}

.gridCtn {
  display: -webkit-flex;
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
  border-top: 1rpx #cac9ce solid;
}

.nm .gridCtn {
  background-color: #000;
  border-top: 1rpx #353631 solid;
}

.gridBox {
  width: 25%;
  flex-grow: 0;
  padding: 0 !important;
  margin: -1rpx 0 0 -1rpx !important;
  border-top: none !important;
  border-bottom: 1rpx #cac9ce solid !important;
  border-left: none !important;
  border-right: 1rpx #cac9ce solid !important;
  border-radius: 0 !important;
  line-height: 1.6 !important;
}

.nm .gridBox {
  border-bottom: 1rpx #353631 solid !important;
  border-right: 1rpx #353631 solid !important;
}

.gridIcon {
  width: 88rpx;
  height: 88rpx;
  margin-top: 32rpx;
}

.gridTitle {
  font-size: 32rpx;
  color: #444;
  margin-bottom: 12rpx;
}

.nm .gridTitle {
  color: #bbb;
}

.gridFoot {
  font-size: 14px;
  color: #999;
  text-align: left;
  padding: 3px 15px;
  min-height: 8px;
}

.nm .gridFoot {
  color: #666;
}

/* swiper */

.swiper {
  width: 100%;
  height: 400rpx;
}

.swiperImg {
  width: 100% !important;
  height: 100% !important;
}

/* audio */

.audio {
  width: 80%;
  margin: 20px 10%;
  overflow: hidden;
  border-radius: 34px;
}

/* error */

.errorCtn {
  height: 70%;
  padding: 20% 10%;
}

.errorText {
  color: red;
  font-size: 18px;
  margin: 10px 0;
}

.errorDesc {
  color: #888;
  font-size: 16px;
  text-align: justify !important;
}

/* list */

