}

.nm .link-divline {
  background-color: #353631;
}

.link-divline:last-child {
  display: none;
}

.pickerViewCtn {
  height: 160px;
}

.pick-view-column {
  min-width: 50px;
  flex: none;
}

.pickColumn {
  display: -webkit-flex;
  display: flex;
  padding: 0 8px;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  font-size: 18px;
  flex: none;
}

.pick-view-column:first-child .pickColumn {
  text-align: right;
}

.pick-view-column:last-child .pickColumn {
  text-align: left;
}

.iOSpick-view-column:only-child .iOSpickColumn {
  text-align: center;
}

.nm .Lctn, .nm .pickerViewCtn, .nm .pick-view-column {
  background-color: #000;
  color: #fff;
}

.sliderCtn {
  padding: 5px 15px;
}

.nm .LinkText, .nm .pickerViewCtn {
