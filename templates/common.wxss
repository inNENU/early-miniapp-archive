}

.LinkText {
  padding: 10px 0;
  line-height: 1.5;
}

.listIcon {
  width: 26px;
  height: 26px;
  margin-right: 10px;
}

.list-Container {
  position: relative;
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
  align-items: center;
}

.list-Container, .LinkText, .pick-view-column:first-child,
.pick-view-column:last-child {
  -webkit-flex: 1;
  flex: 1;
}

.linkDesciption {
  color: #999 !important;
