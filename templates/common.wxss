}

.nm .commonhead {
  color: #fff;
}

.LinkText, .commonhead, .commonfoot {
  text-align: left;
}

.Lctn {
  background-color: #fefefe;
  border-top: 1rpx #cac9ce solid;
  border-bottom: 1rpx #cac9ce solid;
}

.nm .Lctn {
  border-color: #353631;
}

.listCtn {
  padding: 0 15px;
  font-size: 17px;
  line-height: 1.45;
  border-radius: 0;
  border: none !important;
  color: #000 !important;
