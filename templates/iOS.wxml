<!-- iOS theme -->
<template name="iOShead">
  <view class='iOS-navigationBar' style="{{item.iPhoneX?'padding-top:46px;height:40px;':''}}{{item.android?'top:10px;':''}}">
    <view wx:if="{{item.titleDisplay}}" class="iOS-titletext">{{item.title}}</view>
    <button wx:if="{{!item.top}}" class='iOS-navBack' hover-class="iOSnavBackHover" plain='true' bindtap='back'>
      <view class='iOS-backArrow'></view>
      <text class='iOS-nav-text'>{{item.backText}}</text>
    </button>
  </view>
  <view wx:if="{{item.borderDisplay}}" class='iOSnavDl' style="{{item.iPhoneX?'top:86px;':''}}{{item.android?'margin-top:10px;':''}}"></view>
  <view class='iOS-navPlaceHolder' style="{{item.iPhoneX?'height:86px':''}}">
    <text>到头啦~人家被你拉得好痛阿(*´д`*) 不许你再拉人家~╭(╯^╰)╮</text>
  </view>
  <view class='iOS-head'>
    <text>{{item.title}}</text>
  </view>
</template>

<template name='iOSh2'>
  <view class='iOSH2'>{{item.text}}</view>
</template>

<template name='iOSh3'>
  <view class='iOSH3'>{{item.text}}</view>
</template>

<template name='iOSh4'>
  <view class='iOSH4'>{{item.text}}</view>
</template>

<template name='iOSp'>
  <view class='iOSP'>
    <text>{{item.text}}</text>
    <image wx:if="{{item.src}}" class='iOSP-img' lazy-load="true" mode="aspectFill" src="{{item.src}}" />
  </view>
</template>

<template name='iOSlist'>
  <view wx:if="{{item.heading!==false}}" class='iOSLheading'>{{item.heading}}</view>
  <view class='iOSLctn'>
    <block wx:for-items="{{item.content}}" wx:for-item="List" wx:key="{{item.text}}">
      <navigator wx:if='{{List.url}}' class="iOS-listCtn" url='{{List.url}}'>
        <!-- <view>此处插入icon</view> -->
        <view class='iOS-list-Container'>
          <view class='iOSLinkText'>{{List.text}}</view>
          <view class="iOS-linkDesciption {{List.url?'iOSaccess':''}}"> {{List.desc}} </view>
        </view>
      </navigator>
      <view wx:if='{{!List.url && !List.pickerValue}}' class="iOS-listCtn {{List.Switch?'iOS-switchList':''}}">
        <!-- <view>此处插入icon</view> -->
        <view class='iOS-list-Container'>
          <view class='iOSLinkText'>{{List.text}}</view>
          <view class="iOS-linkDesciption">{{List.desc}}
            <switch wx:if='{{List.Switch}}' checked='{{List.checked}}' bindchange="{{List.Switch}}" />
          </view>
        </view>
      </view>
      <view wx:if='{{List.pickerValue}}' class="iOS-listCtn" bindtap='{{List.tap}}'>
        <view class='iOS-list-Container'>
          <view class='iOSLinkText'>{{List.text}}</view>
          <view class="iOS-linkDesciption">
            <block wx:for='{{List.value}}' wx:key>{{item}}</block>
          </view>
        </view>
      </view>
      <block wx:if='{{List.display}}'>
        <view class='iOS-link-divline'></view>
        <picker-view indicator-class='iOSpickSelected' class='iOSpickerViewCtn' value="{{List.currentValue}}" bindchange="{{List.picker}}">
          <block wx:for='{{List.pickerValue}}' wx:key="*this">
            <picker-view-column class='iOSpick-view-column'>
              <view wx:for="{{item}}" wx:key="*this" class='iOSpickColumn'>{{item}}</view>
            </picker-view-column>
          </block>
        </picker-view>
      </block>
      <view class='iOS-link-divline'></view>
    </block>
  </view>
  <view wx:if="{{item.ending!==false}}" class='iOSLending'>{{item.ending}}</view>
</template>

