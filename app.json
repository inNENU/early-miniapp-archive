	"subPackages": [
		{
			"root": "function",
			"pages": [
				"cal",
				"map",
				"situs",
				"player",
				"PEcal",
				"scenery",
				"notice",
				"noticeDetail"
			]
		},
		{
			"root": "modules",
			"pages": [
				"module1",
				"module2",
				"module3",
