<view class="{{T}} {{nm?'nm':''}}">
  <block wx:if="{{contact}}">
    <button
      wx:if="{{appID === 'wx9ce37d9662499df3'}}"
      class="action"
      open-type="contact"
      type="primary"
    >
      联系校会君
    </button>
    <button wx:else type="primary" class="action" open-type="contact">
      联系Mr.Hope
    </button>
  </block>
  <button
    class="action"
    wx:if="{{feedback}}"
    open-type="feedback"
    type="primary"
  >
    意见反馈
  </button>
  <button
    wx:if="{{login}}"
    class="action"
    open-type="getUserInfo"
    type="primary"
  >
    登录
  </button>
  <button type="primary" class="action" wx:if="{{reset}}" catch:tap="resetApp">
    初始化小程序
  </button>
  <navigator wx:if="{{exit}}" open-type="exit" target="miniProgram">
    <button type="primary" class="action">退出小程序</button>
  </navigator>
  <button type="default" class="back" catch:tap="main">返回主页</button>
</view>
