<mp-cells ext-class="mp-cells" title="{{config.head}}" footer="{{config.foot}}">
  <block wx:for="{{config.content}}" wx:for-item="listItem" wx:key="text">
    <!-- 判断是否隐藏列表这一项 -->
    <block wx:if="{{!listItem.hidden}}">
      <mp-cell
        wx:if="{{listItem.url}}"
        icon="{{listItem.icon}}"
        value="{{listItem.text}}"
        footer="{{listItem.desc}}"
        link
        hover
        data-url="{{listItem.url}}"
        catch:tap="navigate"
      />
      <mp-cell wx:else icon="{{listItem.icon}}" footer="{{listItem.desc}}">
        <text class="text" decode selectable space="nbsp"
          >{{listItem.text}}</text
        >
      </mp-cell>
    </block>
  </block>
</mp-cells>
