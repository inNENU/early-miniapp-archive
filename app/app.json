{
  "pages": [
    "page/main/main",
    "page/function/function",
    "page/guide/guide",
    "page/me/me",
    "page/search/search"
  ],
  "subPackages": [
    {
      "root": "function",
      "pages": [
        "cal/cal",
        "map/map",
        "situs/situs",
        "player/player",
        "video/video",
        "PEcal/PEcal",
        "scenery/scenery",
        "weather/weather",
        "calendar/calendar"
      ]
    },
    {
      "root": "module",
      "pages": [
        "module1",
        "module2",
        "module3",
        "module4",
        "module5",
        "module6",
        "module7",
        "sharePage",
        "web",
        "building",
        "function"
      ]
    },
    {
      "root": "settings",
      "pages": [
        "outlook/outlook",
        "storage/storage",
        "auth/auth",
        "about/about",
        "donate/donate",
        "resource/resource",
        "log/log"
      ]
    }
  ],
  "preloadRule": {
    "page/main/main": {
      "network": "all",
      "packages": ["module"]
    },
    "page/function/function": {
      "network": "all",
      "packages": ["function"]
    },
    "page/me/me": {
      "network": "all",
      "packages": ["settings"]
    }
  },
  "window": {
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTxtStyle",
    "navigationBarTitleText": "东师青年",
    "navigationStyle": "custom",
    "backgroundColor": "@bgColor",
    "backgroundTextStyle": "@bgTxtStyle",
    "backgroundColorTop": "@bgColorTop",
    "backgroundColorBottom": "@bgColorBottom",
    "pageOrientation": "portrait"
  },
  "tabBar": {
    "custom": false,
    "color": "#8a8a8a",
    "selectedColor": "#3cc51f",
    "borderStyle": "@tabBorderStyle",
    "backgroundColor": "@tabBgColor",
    "position": "bottom",
    "list": [
      {
        "pagePath": "page/main/main",
        "text": "首页",
        "iconPath": "icon/tab/home.png",
        "selectedIconPath": "icon/tab/@Home.png"
      },
      {
        "pagePath": "page/function/function",
        "text": "功能",
        "iconPath": "icon/tab/function.png",
        "selectedIconPath": "icon/tab/@Function.png"
      },
      {
        "pagePath": "page/guide/guide",
        "text": "东师指南",
        "iconPath": "icon/tab/guide.png",
        "selectedIconPath": "icon/tab/@Guide.png"
      },
      {
        "pagePath": "page/me/me",
        "text": "我的东师",
        "iconPath": "icon/tab/me.png",
        "selectedIconPath": "icon/tab/@Me.png"
      }
    ]
  },
  "networkTimeout": {
    "request": 5000,
    "connectSocket": 8000,
    "uploadFile": 8000,
    "downloadFile": 25000
  },
  "debug": true,
  "resizable": true,
  "permission": {
    "scope.userLocation": {
      "desc": "您的位置信息将用于校园地图导航"
    }
  },
  "requiredBackgroundModes": ["audio"],
  "usingComponents": {
    "doc": "/components/common/doc/doc",
    "ex-list": "/components/common/ex-list/list",
    "foot": "/components/common/foot/foot",
    "grid": "/components/common/grid/grid",
    "gzh": "/components/common/gzh/gzh",
    "img": "/components/common/img/img",
    "intro": "/components/common/intro/intro",
    "list": "/components/common/list/list",
    "media": "/components/common/media/media",
    "p": "/components/common/paragraph/paragraph",
    "phone": "/components/common/phone/phone",
    "share": "/components/common/share/share",
    "my-swiper": "/components/common/swiper/swiper"
  },
  "style": "v2",
  "darkmode": true,
  "themeLocation": "style/theme.json",
  "sitemapLocation": "sitemap.json",
  "useExtendedLib": {
    "weui": true
  }
}
